{
  "id": "marketing-budget-review",
  "title": "Marketing Budget Review Meeting Prep",
  "description": "Analyze marketing campaign budgets and ROI across channels",
  "category": "business",
  "job": "marketing-content",
  "example_prompt": "Review our Q2 marketing budget allocation and ROI across different channels. Identify top performing channels and suggest optimizations for Q3.",
  "extensions": [
    {
      "name": "Google Drive",
      "command": "npx -y @modelcontextprotocol/server-gdrive",
      "is_builtin": false,
      "link": "https://block.github.io/goose/v1/extensions/google-drive",
      "environmentVariables": [
        {
          "name": "GDRIVE_OAUTH_PATH",
          "description": "/Users/<username>/.config/gcp-oauth.keys.json / ",
          "required": true
        },
        {
          "name": "GDRIVE_CREDENTIALS_PATH",
          "description": "/Users/<username>/.config/.gdrive-server-credentials.json /",
          "required": true
        }
      ]
    }
  ]
}
